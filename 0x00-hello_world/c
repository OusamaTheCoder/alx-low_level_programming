# 1 "0-preprocessor"
# 1 "<built-in>"
# 1 "<command-line>"
# 31 "<command-line>"
# 1 "/usr/include/stdc-predef.h" 1 3 4
# 32 "<command-line>" 2
# 1 "0-preprocessor"


if [ -z "$CFILE" ]; then
    echo "CFILE variable is not set. Please set it to the name of your C file."
    exit 1
fi

cpp "$CFILE" > c
